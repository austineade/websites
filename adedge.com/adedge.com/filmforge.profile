/**
* The modules that are enabled when this profile is installed.
*
* @return
*  An array of modules to be enabled.
*/
function filmforge_profile_modules() {
  
  return array(

    /* CCK */
    'content', 'date', 'fieldgroup', 'nodereference', 'number', 'optionwidgets', 'text', 'userreference', 'votingapi_field', 

    /* Core - optional */
    'aggregator', 'color', 'comment', 'contact', 'help', 'locale', 'menu', 'path', 'profile', 'search', 'taxonomy', 'throttle', 'upload', 

    /* Core - required */
    'block', 'filter', 'node', 'system', 'user', 'watchdog', 

    /* Image */
    'image', 'image_attach', 

    /* Licenses */
    'creativecommons_lite', 

    /* Multilanguage - i18n */
    'i18n', 'i18nblocks', 'i18nmenu', 'i18nprofile', 'i18ntaxonomy', 'i18nviews', 'translation', 

    /* Multilanguage - i18n - experimental */
    'i18ncontent', 'i18nstrings', 'i18nsync', 'ttext', 

    /* Other */
    'autolocale', 'date_api', 'diff', 'panels', 'pathauto', 'pearwiki_filter', 'recent_changes', 'wikitools', 

    /* Taxonomy */
    'tagadelic', 

    /* Video */
    'video', 'video_customfields', 'video_ffmpeg_helper', 'video_image', 'video_multidownload', 'video_optmetadata', 'video_params', 'video_upload', 

    /* Views */
    'nodecloud', 'views', 'views_bonus', 'views_rss', 'views_theme_wizard', 'views_ui', 

    /* Voting */
    'fivestar', 'votingapi', 
  
  );
  
}  
  
/**
* Implementation of hook_profile_details().
*
* This contains an array of profile details for display from the main selection screen.
*/
function filmforge_profile_details() {
  return array(
    'name' => st('Filmforge install profile'),
    'description' => st('This install profile installs a video-based community website.')
  );
}
    
  
